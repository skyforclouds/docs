{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "SoraNova Docs",
  "colors": {
    "primary": "#A1A1A1",
    "light": "#7391AB",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              {
                "group": "CLI Quickstart",
                "pages": [
                  "quickstart/model-deployment",
                  "quickstart/deploying-hf-llm",
                  "quickstart/deploying-custom-models",
                  "quickstart/using-openai-sdk"
                ]
              }
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/dsl"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "API Documentation",
            "pages": [
              "api-reference/introduction"
            ]
          },
          {
            "group": "Endpoint Examples",
            "pages": [
              "api-reference/endpoint/completions",
              "api-reference/endpoint/embeddings"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": []
    }
  },
  "logo": {
    "light": "/logo/logo.svg",
    "dark": "/logo/logo.svg",
    "href": "https://soranova.ai"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:saasadmin@soracloud.ai"
      }
    ]
  },
  "footer": {
    "socials": {
    }
  },
  "api": {
    "playground": {
      "display": "none"
    },
    "openapi": {
      "source": "openapi.yaml"
    }
  }
}